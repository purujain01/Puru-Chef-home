{
  "name": "admin-dashboard",
  "version": "1.0.0",
  "description": "Admin dashboard for website",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'admin/*'\" \"npm run admin\"",
    "admin": "cd admin && npm run start",
    "seed": " node scripts/Seeds.js",
    "install": "cd admin && npm install",
    "build": "cd admin && npm run build",
    "heroku-postbuild": " npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.4.5",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jsdoc-swagger": "^1.6.2",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "i": "^0.3.6",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^5.12.13",
    "mongoose-sequence": "^5.3.1",
    "nodemailer": "^6.6.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "razorpay": "^2.0.6",
    "sanitize-html": "^2.4.0",
    "uuid": "^8.2.0",
    "validator": "^13.6.0"
  }
}
